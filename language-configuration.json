{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "surroundingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(kelas|tugas|properti|jika|jikalau|kalau|selagi|untuk|\\w+\\s*=\\s*baru\\s+\\w+\\s*\\().*\\{\\s*$",
    "decreaseIndentPattern": "^\\s*\\}\\s*$"
  }
}